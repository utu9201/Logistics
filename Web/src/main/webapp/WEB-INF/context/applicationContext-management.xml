<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="managementServiceFacade"
		class="net.plang.logistics.base.sf.ManagementServiceFacadeImpl"
		p:managementApplicationService-ref="managementApplicationService"
		/>
		
	<bean id="managementApplicationService"
		class="net.plang.logistics.base.application.ManagementApplicationServiceImpl"
		p:deptDAO-ref="deptDAO"
		p:companyDAO-ref="companyDAO"
		p:workplaceDAO-ref="workplaceDAO"
		p:positionDAO-ref="positionDAO"
		/>
		
	<bean id="deptDAO"
		class="net.plang.logistics.base.dao.DeptDAOImpl"
		parent="iBatisSupportDao"
		/>

	<bean id="companyDAO"
		class="net.plang.logistics.base.dao.CompanyDAOImpl"
		parent="iBatisSupportDao"
		/>
		
	<bean id="workplaceDAO"
		class="net.plang.logistics.base.dao.WorkplaceDAOImpl"
		parent="iBatisSupportDao"
		/>		
	<bean id="positionDAO"
		class="net.plang.logistics.base.dao.PositionDAOImpl"
		parent="iBatisSupportDao"
		/>

</beans>
