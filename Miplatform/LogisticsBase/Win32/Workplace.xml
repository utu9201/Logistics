	<Script><![CDATA[#include "Script::serialTempleteCode.js"
function btn_addDetailCode_OnClick(obj)
{
	var rowNum = gds_workplace.AddRow();
	trace("gg"+global_company);
	gds_workplace.SetColumn(rowNum,"COMPANY_CODE",global_company);
	gds_workplace.SetColumn(rowNum,"WORKPLACE_CODE",templeteCodeNo(gds_workplace,"WORKPLACE_CODE"));
}

function btn_delDetailCode_OnClick(obj)
{
		gds_workplace.DeleteRow(gds_workplace.row);
}

function btn_batchCode_OnClick(obj)
{
	Transaction("registWorkplace","svcManagement::workplace.do?method=batchWorkplaceList","gds_workplace=gds_workplace:u","gds_workplace=gds_workplace","","callback");
    trace(http.SendHttpStr);
}

function callback(trid , ErorrCode , ErorrMsg){
	
	if(trid=="registWorkplace"){
		if(ErorrCode >= 0 ){
			alert("저장완료");
		}
		
	}
	gds_workplace.Editable=false;
	 Reload();
	
}

function edt_workplaceEstablishDate_OnClick(obj){

  var arr = array();
  arr = edt_workplaceEstablishDate.GetSel(); // Grid Cell의 좌표 얻어오기
  Calendar0.Top = edt_workplaceEstablishDate.Top + arr[0]-100;  // Calendar의 위치 선정
  Calendar0.Left = edt_workplaceEstablishDate.Left + arr[1]; // Calendar의 위치 선정
  Calendar0.Visible = true;  // Calendar 나타나게 하기
 
}


function Calendar0_OnChanged(obj,strOldText,strNewText){

edt_workplaceEstablishDate.text=strNewText;
 Calendar0.Visible = false; // Calendar 사라지게 하기

}


function Button0_OnClick(obj)
{
	Dialog ("Base::PostInfoForm.xml",350,350);
}
