	<Script><![CDATA[#include "Script::serialTempleteCode.js"
function Customer_OnLoadCompleted(obj)
{
	
		http.Sync=true;
	
	transaction("findCustomerList",
				"svcBusiness::customer.do?method=findCustomerList",
				"",
				"gds_customer=gds_customer",
				"",
				"callback");
			
	//initSession(false);
	http.Sync=false;	
	

	
	
}
#include "Script::minSearcher.js"
function btn_search_OnClick(obj)
{	
	var customer = txt_search.text;
	minSearcher(customer,"CUSTOMER_NAME",gds_customer);
	
}

function btn_addDetailCode_OnClick(obj)
{

	var rowNum = gds_customer.AddRow();
	gds_customer.SetColumn(rowNum,"CUSTOMER_CODE",templeteCodeNo(gds_customer,"CUSTOMER_CODE"));
	
}

function btn_delDetailCode_OnClick(obj)
{
	gds_customer.DeleteRow(gds_customer.row);
}

function btn_batchCode_OnClick(obj)
{
		Transaction("registcustomer","svcManagement::dept.do?method=batchCustomerList","gds_customer=gds_customer:u","gds_customer=gds_customer","","callback");
       trace(http.SendHttpStr);
}


function callback(trid , ErorrCode , ErorrMsg){
	
	if(trid=="registcustomer"){
		if(ErorrCode >= 0 ){
			alert("저장완료");
		}
		
	}
	 
	
}
function btn_addr_OnClick(obj)
{
	Dialog ("Base::PostInfoForm.xml",350,350);
}
